            <MenuItem onAction="#addDataStore" text="Add data store" />
            <MenuItem onAction="#addExternalEntity" text="Add external entity" />
        </Menu>
    </MenuBar>
    <HBox AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
