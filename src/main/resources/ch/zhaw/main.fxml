                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <children>
                        <Label text="NODE_TYPE" />
                        <HBox prefWidth="200.0" spacing="10.0">
                           <children>
                              <Label text="Text:" />
                              <TextField />
                           </children>
                        </HBox>
                     </children></VBox>
               </content>
            </TitledPane>
         </children>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin></VBox>
      <padding>
         <Insets top="15.0" />
      </padding>
    </HBox>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
    </padding>
</AnchorPane>
