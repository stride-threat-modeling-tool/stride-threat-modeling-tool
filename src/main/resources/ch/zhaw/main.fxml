               <content>
                    <VBox minHeight="300.0" minWidth="266.66" prefHeight="500.0" prefWidth="300.0" spacing="10.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <children>
                        <Label onMouseClicked="#addDataStore" text="Data Store" />
                        <Label onMouseClicked="#addExternalEntity" text="External Entity" />
